#! /bin/sh
# $Id$
# shell script to figure out architecture, using C preprocessor
# works on the architectures we support.
arch=`echo "
#ifdef sun
#ifdef sparc
Xsun4
#else
Xsun3
#endif
#endif
#ifdef vax
Xvax
#endif
#ifdef mips
#ifdef sgi
Xirix4
#else
Xmips
#endif
#endif
#ifdef hppa
Xhppa
#endif
" | /lib/cpp  | grep '^X' | sed s/X// `
if [ $arch = "irix4" ]; then
	uname -r | awk '{split($1,f,"."); print "irix"f[1]}'
else	
	echo $arch
fi

