# Tycho preferences file to use small fonts.
#
# $Id$

::tycho::preference assign fonts textFont  {Screen 12}
::tycho::preference assign fonts entryFont {Helvetica 12}
::tycho::preference assign fonts menuFont  {Helvetica 12}

::tycho::preference assign layout textWidth 75
::tycho::preference assign layout textHeight 50

::tycho::preference assign viewhtml fontSize small
::tycho::preference assign viewhtml textWidth 75
::tycho::preference assign viewhtml textHeight 50

