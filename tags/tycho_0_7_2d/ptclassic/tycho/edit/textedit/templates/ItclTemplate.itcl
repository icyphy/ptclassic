#
body ::tycho::ClassName::publicProcA {anyargs} {
}


    ###################################################################
    ###################################################################
    ####                      protected methods                    ####

#######################################################################
#### MethodName
# Description.
#
body ::tycho::ClassName::_protectedMethodA {} {
}

    ###################################################################
