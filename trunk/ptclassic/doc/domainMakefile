# $Id$
#
# Makefile for domain sections.  Each domain should make a symbolic
# link from this file to $(PTOLEMY)/src/domains/doc/Makefile.

PTOLEMY       = ../../../..
PRINTSEC      = $(PTOLEMY)/doc/bin/printsec
HEADER        = $(PTOLEMY)/doc/headers/together $(PTOLEMY)/doc/headers/sogod
MAINSEC	      = overall
TMPFILE	      = tmp_print
STARDIRS      = stars
DEMODIRS      = demos
OPTIONS       =

MAKEVARS =	\
		"PTOLEMY=$(PTOLEMY)" \
		"PRINTER=$(PRINTER)" \
		"PRINTSEC=$(PRINTSEC)" \
		"HEADER=$(HEADER)"

print:
	$(PTOLEMY)/doc/bin/printdom $(OPTIONS)

preview:
	$(PTOLEMY)/doc/bin/printdom -preview $(OPTIONS)
