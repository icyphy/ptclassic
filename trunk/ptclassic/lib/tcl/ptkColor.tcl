# Look up a color.  Convert all characters to lower case, so
# that case does not matter.
proc ptkColor name {
    set lname [string tolower $name]
    global ptkColorList
    if [info exists ptkColorList($lname)] {
        return $ptkColorList($lname)
    } else {
        return $ptkColorList(black)
    }
}
